<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        <hibernate-mapping>
        <class name="cn.jx.pxc.entity.sights" table="t_sights">
           <id name="trId" column="trId">
               <generator class="native"></generator>
           </id>
             <property name="trName" column="trName"></property>
             <property name="trIntro" column="trIntro"></property>
             <property name="trImage" column="trImage"></property>
             <property name="trType" column="trType"></property>
             <property name="trSatisfied" column="trSatisfied"></property>
             <property name="trPrice" column="trPrice"></property>
             <property name="trPromotionPrice" column="trPromotionPrice"></property>
             <property name="trTravelAmount" column="trTravelAmount"></property>
             <property name="trAccessAmount" column="trAccessAmount"></property>
             <property name="trRemarkAmount" column="trRemarkAmount"></property>
             <property name="trAttributionPlace" column="trAttributionPlace"></property>
             <property name="createdTime" column="createdTime"></property>
             <property name="createdUser" column="createdUser"></property>
             <property name="modefiedTime" column="modefiedTime"></property>
             <property name="modefiedUser" column="modefiedUser"></property>
        </class>      
        </hibernate-mapping>